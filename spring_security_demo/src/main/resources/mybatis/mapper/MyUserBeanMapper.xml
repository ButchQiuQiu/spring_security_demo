<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.butch.spring_security_demo.mapper.MyUserBeanMapper">
  <select id="selectByUsername" resultType="MyUserBean" parameterType="string">
    select * from user where username = #{username}
  </select>
</mapper>